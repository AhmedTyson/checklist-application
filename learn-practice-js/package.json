{
  "name": "synapse",
  "productName": "Synapse",
  "version": "1.0.0",
  "description": "A personal learning command center for tracking progress and mastering new technologies.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . --overwrite --platform=win32 --arch=x64 --prune=true --out=release-builds",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --prune=true --out=release-builds"
  },
  "keywords": [
    "roadmap",
    "education",
    "electron",
    "learning-tracker",
    "curriculum"
  ],
  "author": "Ahmed Tyson",
  "license": "MIT",
  "devDependencies": {
    "electron": "^33.0.0",
    "electron-packager": "^17.1.2"
  }
}
